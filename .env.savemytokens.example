# Free Agents API Keys
# Copy this file to .env.savemytokens and add your FREE API keys

# Cerebras (Recommended - Fast & Generous Free Tier)
# Get your free key: https://cerebras.ai
CEREBRAS_API_KEY=your_cerebras_key_here
CEREBRAS_MODEL=llama-3.3-70b

# Mistral (Good Free Tier)
# Get your free key: https://mistral.ai
MISTRAL_API_KEY=your_mistral_key_here
MISTRAL_MODEL=mistral-small-latest

# Optional: Caching (save even more API calls!)
FREE_AGENTS_CACHE_ENABLED=true
FREE_AGENTS_CACHE_DIR=./cache
